	measure TotalPopuNonAccesServicesSafely =
			
			SUMX(
			    VALUES(DIM_region_country[Country]),
			    VAR PopPays = CALCULATE(AVERAGE(FAIT_drinking_water_services[population]))
			    VAR TauxPays = CALCULATE(AVERAGE(FAIT_drinking_water_services[popu_use_safely_drinking_water_services])) / 100
			    RETURN PopPays - (PopPays * TauxPays)
			)
		displayFolder: Accès à l'eau
