	measure UrbanPopulationRate =
			
			DIVIDE(
			    CALCULATE(
			        AVERAGE(FAIT_population[Population]),
			        REMOVEFILTERS(FAIT_population[Granularity]),
			        KEEPFILTERS(FAIT_population[Granularity] = "Urban")
			    ),
			    CALCULATE(
			        AVERAGE(FAIT_population[Population]),
			        REMOVEFILTERS(FAIT_population[Granularity]),
			        KEEPFILTERS(FAIT_population[Granularity] = "Total")
			    )
